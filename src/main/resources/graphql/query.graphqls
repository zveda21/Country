scalar Date

type Query {
    country(id : ID!) : Country!
    countryByCode(countryCode: String!): Country!
    countries : [Country!]
}

type Mutation{
    addCountry( input : CountryInput!) : Country!
    updateCountryName(countryCode: String!, newCountryName: String!) : Country!
}

type Country {
    id : ID!
    countryName : String
    countryCode : String!
    independenceDate: Date
}

input CountryInput {
    countryName : String!
    countryCode : String!
}